{
    "parser": "babel-eslint",
    "rules": {
        "indent": [2, 4],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "strict": 0,
        "prefer-const": [2],
        "no-underscore-dangle": 0,
        "no-shadow": 0,
        "no-unused-vars": [2, { "vars": "all", "args": "none" }],
        "no-alert": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "classes": true
    },
    "globals": {
        "window": true,
        "$": true,
        "fabric": true,
        "env": true
    }
}
